{
    "name": "shopify-product-importer",
    "version": "1.0.0",
    "description": "Modern Shopify app for importing products from CJ and Pepperjam APIs",
    "main": "dist/server/index.js",
    "packageManager": "pnpm@10.11.0",
    "scripts": {
        "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\"",
        "dev:server": "nodemon --exec ts-node -r tsconfig-paths/register server/index.ts",
        "dev:client": "vite",
        "build": "pnpm run build:server && pnpm run build:client",
        "build:server": "tsc -p server/tsconfig.json",
        "build:client": "vite build",
        "start": "node dist/server/index.js",
        "start:prod": "NODE_ENV=production node dist/server/index.js",
        "deploy": "./scripts/deploy.sh",
        "prod:check": "./scripts/production-check.sh",
        "prod:migrate": "npx prisma migrate deploy",
        "lint": "eslint . --ext .ts,.tsx",
        "type-check": "tsc --noEmit",
        "test": "jest",
        "clean": "rimraf dist node_modules/.cache",
        "postinstall": "prisma generate",
        "db:seed": "tsx prisma/seed.ts"
    },
    "dependencies": {
        "@prisma/client": "^6.8.2",
        "@shopify/admin-api-client": "^1.0.1",
        "@shopify/app-bridge": "^3.7.10",
        "@shopify/polaris": "^13.9.5",
        "@shopify/polaris-icons": "^9.3.1",
        "@shopify/shopify-api": "11.12.0",
        "axios": "^1.6.2",
        "bull": "^4.12.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^5.1.0",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.2",
        "node-cron": "^4.0.7",
        "prisma": "^6.8.2",
        "redis": "^5.1.0",
        "winston": "^3.11.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@tanstack/react-query": "^5.12.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.2",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^22.15.21",
        "@types/react": "^19.1.5",
        "@types/react-dom": "^19.1.5",
        "@typescript-eslint/eslint-plugin": "^8.32.1",
        "@typescript-eslint/parser": "^8.32.1",
        "@vitejs/plugin-react": "^4.2.1",
        "concurrently": "^9.1.2",
        "eslint": "^9.27.0",
        "nodemon": "^3.0.2",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "rimraf": "^6.0.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.19.4",
        "typescript": "^5.3.3",
        "vite": "^6.3.5"
    },
    "engines": {
        "node": ">=18.0.0",
        "pnpm": ">=10.0.0"
    },
    "keywords": [
        "shopify",
        "app",
        "product-import",
        "cj",
        "pepperjam",
        "affiliate"
    ],
    "author": "Your Name",
    "license": "MIT"
}